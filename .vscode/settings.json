{
  "php.inlayHints.parameters.enabled": false,
  "php.version": "8.5",
  "[php]": {
    "editor.defaultFormatter": "DEVSENSE.phptools-vscode",
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "[blade]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true
  },
  "files.associations": {
    "*.php": "php",
    "*.blade.php": "blade"
  }
}
